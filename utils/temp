#!/usr/bin/env python3

import sys


def _temp(s):
    v = float(s)
    f, c = v * 1.8 + 32, (v - 32) / 1.8
    print(f"{v:.1f}째F={c:.1f}째C\t{v:.1f}째C={f:.1f}째F")


if __name__ == "__main__":
    if len(sys.argv) > 1:
        for s in sys.argv[1:]:
            _temp(s)
    else:
        for line in sys.stdin:
            _temp(line)
